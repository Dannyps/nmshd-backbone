{
    "Logging": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Backbone": "Information",
                "Enmeshed": "Information",
                "ConsumerApi": "Information",

                "Microsoft": "Information",
                "Microsoft.AspNetCore": "Warning"
            }
        },
        "WriteTo": {
            "Console": {
                "Name": "Console",
                "Args": {
                    "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
                }
            }
        }
    }
}
