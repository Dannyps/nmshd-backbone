<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>Enable</Nullable>
		<AssemblyName>Backbone.Modules.$(MSBuildProjectName)</AssemblyName>
		<RootNamespace>$(AssemblyName.Replace(" ", "_"))</RootNamespace>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Enmeshed.DevelopmentKit.Identity" Version="1.0.3" />
		<PackageReference Include="Enmeshed.Tooling" Version="1.1.2" />
		<PackageReference Include="Microsoft.AspNetCore.Identity.EntityFrameworkCore" Version="6.0.9" />
	</ItemGroup>

</Project>
